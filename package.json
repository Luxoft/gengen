{
  "name": "gengen",
  "version": "1.0.0-rc.1",
  "description": "Tool for generating models and Angular services based on OpenAPIs and Swagger's JSON",
  "bin": {
    "gengen": "./bin/index.js"
  },
  "scripts": {
    "local": "npm i -g ./",
    "build": "tsc -p tsconfig.json",
    "build:all": "npm run build && ncp ./src/templates ./lib/templates",
    "generate-facade": "node ./bin/index.js g:f --file=./swagger.json",
    "generate": "node ./bin/index.js g --file=./swagger.json --output=./.output/selected",
    "generate:all": "node ./bin/index.js g --all --file=./swagger.json --output=./.output/all",
    "debug": "node --inspect-brk ./bin/index.js g --all --file=./swagger.json --output=./dist",
    "check-snapshot": "npm run generate && npm run generate:all && ts-node ./scripts/test-snapshot.ts",
    "test": "npx jest"
  },
  "keywords": [
    "swagger",
    "open-api",
    "codegen",
    "angular"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "4.1.1",
    "ts-node": "8.3.0",
    "prettier": "1.18.2",
    "ejs": "2.7.1",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@types/ejs": "2.6.3",
    "@types/fs-extra": "8.0.0",
    "@types/jest": "26.0.15",
    "@types/node": "12.7.5",
    "@types/node-fetch": "2.5.0",
    "@types/prettier": "1.18.2",
    "fs-extra": "8.1.0",
    "jest": "26.6.3",
    "ncp": "2.0.0",
    "snapshot-diff": "0.8.1",
    "ts-jest": "26.4.4",
    "typescript": "4.0.3"
  }
}
