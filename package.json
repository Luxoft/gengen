{
  "name": "gengen",
  "version": "1.0.0-rc.1",
  "description": "Tool for generating models and Angular services based on OpenAPIs and Swagger's JSON",
  "bin": {
    "gengen": "./bin/index.js"
  },
  "scripts": {
    "local": "npm i -g ./",
    "build": "tsc -p tsconfig.json",
    "build:all": "npm run build && ncp ./src/templates ./lib/templates",
    "generate-config": "node ./bin/index.js g:c --file=./swagger.json",
    "generate": "node ./bin/index.js g --file=./swagger.json --output=./.output/selected",
    "generate:all": "node ./bin/index.js g --all --file=./swagger.json --output=./.output/all",
    "debug": "node --inspect-brk ./bin/index.js g --all --file=./swagger.json --output=./dist",
    "e2e": "npm run generate && npm run generate:all && ts-node ./e2e/e2e.ts",
    "test": "jest"
  },
  "keywords": [
    "swagger",
    "open-api",
    "codegen",
    "angular"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "6.2.0",
    "ts-node": "9.0.0",
    "prettier": "2.1.2",
    "ejs": "3.1.5",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@types/ejs": "3.0.5",
    "@types/fs-extra": "9.0.4",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.7",
    "@types/node-fetch": "2.5.7",
    "@types/prettier": "2.1.5",
    "fs-extra": "9.0.1",
    "jest": "26.6.3",
    "ncp": "2.0.0",
    "snapshot-diff": "0.8.1",
    "ts-jest": "26.4.4",
    "typescript": "4.0.5",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.3"
  }
}
