{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "outDir": "./lib",
    "declaration": true,
    "lib": [
      "es2021",
      "dom"
    ]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "./src/generated/**",
    "./lib/**",
    "./bin/**",
  ]
}
